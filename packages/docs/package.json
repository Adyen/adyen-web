{
    "name": "@adyen/adyen-web-docs",
    "private": true,
    "keywords": [
        "adyen",
        "adyen-web",
        "checkout",
        "payment",
        "payments",
        "components"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "homepage": "https://docs.adyen.com/checkout",
    "repository": "github:Adyen/adyen-web",
    "bugs": {
        "url": "https://support.adyen.com/"
    },
    "scripts": {
        "docs:parse": "typedoc src/docs.ts  --tsconfig ./tsconfig.json --json dist/docs.json --excludePrivate --excludeProtected --excludeInternal",
        "docs:generate": "node src/index.js",
        "docs:clean": "rimraf ./dist && mkdir ./dist",
        "build": "yarn docs:clean && yarn docs:parse && yarn docs:generate",
        "test": "tsc src/docs.ts --allowJs --declaration"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "concurrently": "^5.3.0",
        "prettier": "^1.19.1",
        "typedoc": "^0.20.16",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@adyen/adyen-web": "3.20.0"
    }
}
