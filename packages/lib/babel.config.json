{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true,
        "jsxPragma": "h",
        "jsxPragmaFrag": "Fragment"
      }
    ],
    [
      "@babel/preset-react",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ],
    [
      "@babel/preset-env",
      {
        "useBuiltIns": false,
        "targets": {
          "esmodules": true
        },
        "bugfixes": true
      }
    ]
  ]
}