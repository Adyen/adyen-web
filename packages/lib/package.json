{
    "name": "@adyen/adyen-web",
    "keywords": [
        "adyen",
        "adyen-web",
        "checkout",
        "payment",
        "payments",
        "components"
    ],
    "main": "dist/cjs/index.js",
    "module": "dist/es/index.js",
    "umd:main": "dist/adyen.js",
    "browser": "dist/adyen.js",
    "types": "dist/types",
    "typings": "dist/types",
    "exports": {
        ".": {
            "import": "./dist/es/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./modern": "./dist/es.modern/index.js",
        "./dist/adyen.css": "./dist/adyen.css",
        "./package.json": "./package.json"
    },
    "version": "5.23.0",
    "license": "MIT",
    "homepage": "https://docs.adyen.com/checkout",
    "repository": "github:Adyen/adyen-web",
    "bugs": {
        "url": "https://support.adyen.com/"
    },
    "scripts": {
        "start": "npm run dev-server",
        "dev-server": "cross-env NODE_ENV=development rollup --watch --config config/rollup.config.js",
        "docs:generate": "typedoc --out docs src --exclude \"**/*+(index|.test).ts\"",
        "build": "rm -rf dist/ && npm run type-check-generate && cross-env NODE_ENV=production rollup --config config/rollup.config.js",
        "build:analyze": "rm -rf dist/ && cross-env NODE_ENV=analyze rollup --config config/rollup.config.js",
        "test": "jest --config config/jest.config.js",
        "test:watch": "npm run test -- --watchAll",
        "test:coverage": "npm run test -- --coverage",
        "size": "npm run build && node ./scripts/size",
        "size-only": "node ./scripts/size",
        "type-check": "tsc --noEmit",
        "type-check-generate": "tsc --emitDeclarationOnly",
        "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
        "lint:fix": "npm run lint -- --fix",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@babel/runtime-corejs3": "^7.16.3",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.3",
        "@rollup/plugin-eslint": "^8.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-replace": "^4.0.0",
        "@testing-library/preact": "^2.0.1",
        "@testing-library/preact-hooks": "^1.1.0",
        "@types/jest": "^27.4.1",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "autoprefixer": "^10.4.2",
        "babel-jest": "^27.3.1",
        "cross-env": "^7.0.3",
        "cssnano": "^5.0.6",
        "dotenv": "^10.0.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-preact-pure": "^3.1.0",
        "esbuild": "^0.14.27",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-tsdoc": "^0.2.14",
        "filesize": "^8.0.6",
        "gzip-size": "^6.0.0",
        "jest": "^26.6.3",
        "node-sass": "^6.0.1",
        "postcss": "^8.4.7",
        "postcss-reporter": "^7.0.5",
        "prettier": "^1.19.1",
        "rollup": "^2.70.1",
        "rollup-plugin-esbuild": "^4.5.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-visualizer": "^5.5.1",
        "stylelint": "^13.13.1",
        "stylelint-config-recommended": "^5.0.0",
        "ts-jest": "^26.5.6",
        "tslib": "^2.3.1",
        "typescript": "^4.4.4",
        "whatwg-fetch": "^3.6.2"
    },
    "dependencies": {
        "@babel/runtime": "^7.15.4",
        "@babel/runtime-corejs3": "^7.16.3",
        "@types/applepayjs": "^3.0.4",
        "@types/googlepay": "^0.6.2",
        "classnames": "^2.3.1",
        "core-js-pure": "^3.18.1",
        "preact": "^10.5.13"
    },
    "files": [
        "dist",
        "LICENSE",
        "README"
    ]
}
