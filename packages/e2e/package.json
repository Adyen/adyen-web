{
    "name": "@adyen/adyen-web-e2e",
    "private": true,
    "keywords": [
        "adyen",
        "adyen-web",
        "checkout",
        "payment",
        "payments",
        "components"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "homepage": "https://docs.adyen.com/checkout",
    "repository": "github:Adyen/adyen-web",
    "bugs": {
        "url": "https://support.adyen.com/"
    },
    "scripts": {
        "start": "npm run dev-server",
        "dev-server": "cross-env NODE_ENV=test webpack-dev-server --config app/config/webpack.config.js",
        "test:e2e": "concurrently --kill-others --success first  --names \"app,e2e\" \"npm run start\" \"node tests/index.js\"",
        "test:e2e-remote": "concurrently --kill-others --success first --names \"app,e2e\" \"npm run start\" \"node tests/index.js --remote\""
    },
    "devDependencies": {
        "@adyen/adyen-web-server": "1.0.0",
        "cross-env": "^7.0.3",
        "concurrently": "^6.2.0",
        "css-loader": "^5.2.6",
        "dotenv": "^10.0.0",
        "html-webpack-plugin": "^4.5.2",
        "node-sass": "^6.0.0",
        "prettier": "^1.19.1",
        "sass-loader": "^10.1.1",
        "source-map-loader": "^1.1.3",
        "style-loader": "^2.0.0",
        "testcafe": "~1.13.0",
        "ts-jest": "^26.5.6",
        "ts-loader": "^8.1.0",
        "typescript": "^4.3.2",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2",
        "whatwg-fetch": "^3.6.2"
    },
    "dependencies": {
        "@adyen/adyen-web": "4.6.0"
    }
}
